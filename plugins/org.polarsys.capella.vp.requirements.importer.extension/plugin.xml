<plugin>
   <extension-point id="attributesProvider" name="Attributes Provider" schema="schema/attributesProvider.exsd"/>
    <extension
          point="org.polarsys.capella.vp.requirements.importer.extension.attributesProvider">
       <attributesProvider
       		 id="reqif"
             category="ReqIF standard attributes"
             mandatory="true">
          <attribute
          		name="ReqIF.ChapterName"
                defaultValue="true">
          </attribute>
          <attribute
          		name="ReqIF.Description"
                defaultValue="true">
          </attribute>
          <attribute
          		name="ReqIF.ForeignCreatedBy"
                defaultValue="true">
          </attribute>
          <attribute
          		name="ReqIF.ForeignCreatedThru"
                defaultValue="true">
          </attribute>
          <attribute
          		name="ReqIF.ForeignID"
                defaultValue="true">
          </attribute>
          <attribute
          		name="ReqIF.ForeignModifiedBy"
                defaultValue="true">
          </attribute>
          <attribute
          		name="ReqIF.Prefix"
                defaultValue="true">
          </attribute>
          <attribute
          		name="ReqIF.Text"
                defaultValue="true">
          </attribute>
       </attributesProvider>
    </extension>
</plugin>
